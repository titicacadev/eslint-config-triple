{
  "name": "@titicaca/eslint-config-triple",
  "version": "3.1.0",
  "description": "Triple's ESLint config, following our styleguide",
  "main": "index.js",
  "files": [
    "rules",
    "index.js",
    "prettierrc.js",
    "stylelint.js",
    "create-config.js",
    "frontend.js"
  ],
  "scripts": {
    "lint:es": "eslint '**/*.js'",
    "lint:es:fix": "npm run lint:es -- --fix",
    "prettier": "prettier '**/*.{json,yaml,md}' --check",
    "prettier:fix": "prettier '**/*.{json,yaml,md}' --write",
    "lint": "npm run lint:es; npm run prettier",
    "lint:fix": "npm run lint:es:fix; npm run prettier:fix",
    "update-snapshot": "jest -u",
    "test:watch": "jest --watch",
    "test": "jest",
    "prepare": "[ $CI ] || husky install"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/titicacadev/eslint-config-triple.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "triple",
    "javascript",
    "typescript",
    "styleguide",
    "es2015",
    "es2016",
    "es2017",
    "es2018"
  ],
  "author": "Triple Corp",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/titicacadev/eslint-config-triple/issues"
  },
  "homepage": "https://github.com/titicacadev/eslint-config-triple",
  "engines": {
    "node": ">= 6"
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.18.2",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-jsx": "^11.0.0",
    "eslint-import-resolver-typescript": "^3.1.0",
    "postcss-syntax": "^0.36.2",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-config-styled-components": "^0.1.1"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "*",
    "eslint": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-jsx-a11y": "*",
    "eslint-plugin-node": "*",
    "eslint-plugin-promise": "*",
    "eslint-plugin-react": "*",
    "eslint-plugin-react-hooks": "*",
    "prettier": "*",
    "stylelint": "*"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "eslint": "^8.18.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^7.0.2",
    "jest": "^27.3.1",
    "lint-staged": "^11.2.3",
    "typescript": "^4.7.4"
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": [
      "eslint"
    ],
    "**/*.{json,yaml,md}": [
      "prettier --check"
    ]
  }
}
